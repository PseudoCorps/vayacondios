jgrab() {
    jobdate=$1
    prefix=$2
    mkdir -p $jobdate/_logs
    s3cmd get -r $prefix/$jobdate/_logs/ $jobdate/_logs/
}

jhgrab() {
    jobdate=$1
    prefix=$2
    mkdir -p $jobdate/_logs
    hdp-get $prefix/$jobdate/_logs/ $jobdate/
}

jparse() {
    jobdate=$1
    $dir/job_log_parser.rb $jobdate >$jobdate.json
}

jgrab_and_jparse() {
    jobdate=$1
    jgrab $jobdate; parse $jobdate
}
